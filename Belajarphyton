i = 1
while i <= 10: 
 print(i)
 i += 1

f = 1
while f <= 6: 
 print(i) 
 if f == 3:
     break
 f += 1

k = 0
while i < 6:
    k += 1
    if k == 3:
        continue 
    print(k)
